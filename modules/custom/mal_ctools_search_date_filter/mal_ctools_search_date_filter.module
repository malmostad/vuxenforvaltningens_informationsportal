<?php

/**
 * @file
 * Code for the facet start date feature.
 */

/**
 * Declare ctools plugin directory.
 */
function mal_ctools_search_date_filter_ctools_plugin_directory($owner, $plugin_type) {
  if ($owner == 'ctools' && $plugin_type == 'content_types') {
    return 'plugins/' . $plugin_type;
  }
}
