<?php

/**
 * @file
 * Code for the separate facet feature.
 */

/**
 * Implements hook_ctools_plugin_directory().
 */
function mal_ctools_separate_facet_ctools_plugin_directory($owner, $plugin_type) {
  if ($owner == 'ctools' && $plugin_type == 'content_types') {
    return 'plugins/' . $plugin_type;
  }
}
